ixableErrorCount":0,"fixableWarningCount":0,"source":"79","usedDeprecatedRules":"44"},{"filePath":"80","messages":"81","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"44"},{"filePath":"82","messages":"83","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"84","messages":"85","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/index.js",[],["86","87"],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/reportWebVitals.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/App.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/site/Footer.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/site/Header.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/site/Sidebar.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/site/Home.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/site/Resources.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/FunctionalComponentDemo.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/JSXRules.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/State.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/Effects.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/PropsDemo.js",["88","89","90"],"import React, { useState } from 'react';\nimport PropTypes from 'prop-types';\n\nconst PropsDemo = () => {\n    const [color, setColor] = useState('white');\n    const [backgroundColor, setBackgroundColor] = useState('purple');\n    const [borderRadius, setBorderRadius] = useState('5px');\n    const [borderStyle, setBorderStyle] = useState('dashed');\n    const [display, setDisplay] = useState('inline-block');\n    const [width, setWidth] = useState('350px');\n    const [textAlign, setTextAlign] = useState('center');\n\n    let styles = {\n        color: color,\n        backgroundColor: backgroundColor,\n        borderRadius: borderRadius,\n        borderStyle: borderStyle,\n        display: display,\n        width: width,\n        textAlign: textAlign\n    }\n\n    const toggleColor = () => {\n        color === 'white' ? setColor('pink') : setColor('white');\n    }\n\n    const toggleBackgroundColor = () => {\n        backgroundColor === 'purple' ? setBackgroundColor('#191970') : setBackgroundColor('purple');\n    }\n\n    const toggleBorderRadius = () => {\n        borderRadius === '5px' ? setBorderRadius('20px') : setBorderRadius('5px');\n    }\n\n    const toggleBorderStyle = () => {\n        borderStyle === 'dashed' ? setBorderStyle('double') : setBorderStyle('dashed');\n    }\n\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <div style={styles}>\n                    <FunctionalComponent\n                        string=\"Will this display?\"\n                        function={toggleColor}\n                        selectedStyle={color}\n                    />\n                    <FunctionalComponent\n                        string=\"Props are pretty cool, right?!\"\n                        function={toggleBackgroundColor}\n                        selectedStyle={backgroundColor}\n                    />\n                    <FunctionalComponent\n                        string=\"You can send data from one component ...\"\n                        function={toggleBorderRadius}\n                        selectedStyle={borderRadius}\n                    />\n                    <FunctionalComponent\n                        string=\"... to another!\"\n                        function={toggleBorderStyle}\n                        selectedStyle={borderStyle}\n                    />\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default PropsDemo;\n\nconst FunctionalComponent = (props) => {\n    return (\n        <div>\n            <p>{props.string}</p>\n            <button onClick={props.function}>Press Me!</button>\n            <TinyComponent selectedStyle={props.selectedStyle} />\n        </div>\n    );\n};\n\nconst TinyComponent = (props) => {\n    return (\n        <div>\n            <p>The current style: {props.selectedStyle} </p>\n        </div>\n    );\n};\n\nFunctionalComponent.defaultProps = {\n    string: 'This is wild!',\n    function: () => console.log('Can I see this in my dev tools?'),\n    selectedStyle: 'what style??'\n};\n\nFunctionalComponent.propTypes = {\n    string: PropTypes.string.isRequired,\n    function: PropTypes.func.isRequired,\n    selectedStyle: PropTypes.string.isRequired\n};","/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/Hooks.js",["91"],"import React, { useState, useEffect } from 'react';\n\nconst Hooks = () => {\n    const [query, setQuery] = useState('');\n    const [results, setResults] = useState({});\n\n    const fetcher = () => {\n        fetch(`https://swapi.co/api/people/${query}`)\n            .then(res => res.json())\n            .then(json => {\n                console.log(json)\n                setResults(json);\n            })\n    }\n\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <input value={query} onChange={e => setQuery(e.target.value)} placeholder=\"enter your star wars character number\" />\n                <button onClick={() => fetcher()}>Click for Character!</button>\n                {results ? <h2>{results.name}</h2> : <div></div>}\n            </div>\n        </div>\n    );\n};\n\n\nconst Hooks2 = () => {\n    const [results, queryNum, setQueryNum] = useNumHook('');\n    const [clicks, setClicks] = useClicks(0);\n\n    return (\n        <div className=\"main\">\n            <div className=\"mainDiv\">\n                <h3>Enter a number below to see a number fact.</h3>\n                <input value={queryNum} onChange={e => setQueryNum(e.target.value)} placeholder=\"enter a number\" />\n                {results ? <h2>{results}</h2> : <div></div>}\n                <button onClick={() => setClicks(clicks + 1)}>Click to update document title</button>\n            </div>\n        </div>\n    );\n};\n\nconst useNumHook = (num) => {\n    const [queryNum, setQueryNum] = useState(num);\n    const [results, setResults] = useState('');\n\n    useEffect(() => {\n        if (queryNum !== '') {\n            fetch(`http://numbersapi.com/${queryNum}`)\n                .then(res => res.text())\n                .then(json => {\n                    console.log(json);\n                    setResults(json);\n                });\n        };\n    }, [queryNum]);\n\n    return [results, queryNum, setQueryNum];\n};\n\nconst useClicks = (initCount) => {\n    const [clicks, setClicks] = useState(initCount);\n\n    useEffect(() => {\n        document.title = `You have clicked ${clicks} times`;\n    }, [clicks])\n    return [clicks, setClicks]\n}\n\nexport default Hooks2;","/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/timer-apps/TimePiecesApp.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/timer-apps/TimerApp.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/timer-apps/ClockApp.js",["92","93","94"],"import React, { useState, useEffect } from 'react';\n\nconst ClockApp = () => {\n    return (\n        <div>\n            <h1 className=\"section-title\">React Clock</h1>\n        </div>\n    );\n}","/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/timer-apps/StopWatchApp.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/nyt-app/NytApp.js",[],"/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/apps/nyt-app/NytResults.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":9,"column":21,"nodeType":"101","messageId":"102","endLine":9,"endColumn":31},{"ruleId":"99","severity":1,"message":"103","line":10,"column":19,"nodeType":"101","messageId":"102","endLine":10,"endColumn":27},{"ruleId":"99","severity":1,"message":"104","line":11,"column":23,"nodeType":"101","messageId":"102","endLine":11,"endColumn":35},{"ruleId":"99","severity":1,"message":"105","line":3,"column":7,"nodeType":"101","messageId":"102","endLine":3,"endColumn":12},{"ruleId":"99","severity":1,"message":"106","line":1,"column":17,"nodeType":"101","messageId":"102","endLine":1,"endColumn":25},{"ruleId":"99","severity":1,"message":"107","line":1,"column":27,"nodeType":"101","messageId":"102","endLine":1,"endColumn":36},{"ruleId":"99","severity":1,"message":"108","line":3,"column":7,"nodeType":"101","messageId":"102","endLine":3,"endColumn":15},"no-native-reassign",["109"],"no-negated-in-lhs",["110"],"no-unused-vars","'setDisplay' is assigned a value but never used.","Identifier","unusedVar","'setWidth' is assigned a value but never used.","'setTextAlign' is assigned a value but never used.","'Hooks' is assigned a value but never used.","'useState' is defined but never used.","'useEffect' is defined but never used.","'ClockApp' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]