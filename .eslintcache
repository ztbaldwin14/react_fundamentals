orderRadius}\n                    />\n                    <FunctionalComponent\n                        string=\"... to another!\"\n                        function={toggleBorderStyle}\n                        selectedStyle={borderStyle}\n                    />\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default PropsDemo;\n\nconst FunctionalComponent = (props) => {\n    return (\n        <div>\n            <p>{props.string}</p>\n            <button onClick={props.function}>Press Me!</button>\n            <TinyComponent selectedStyle={props.selectedStyle} />\n        </div>\n    );\n};\n\nconst TinyComponent = (props) => {\n    return (\n        <div>\n            <p>The current style: {props.selectedStyle} </p>\n        </div>\n    );\n};\n\nFunctionalComponent.defaultProps = {\n    string: 'This is wild!',\n    function: () => console.log('Can I see this in my dev tools?'),\n    selectedStyle: 'what style??'\n};\n\nFunctionalComponent.propTypes = {\n    string: PropTypes.string.isRequired,\n    function: PropTypes.func.isRequired,\n    selectedStyle: PropTypes.string.isRequired\n};","/Users/apple/Documents/ElevenFiftyProjects/blueBadge/react_fundamentals/src/components/concepts/Hooks.js",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":9,"column":21,"nodeType":"72","messageId":"73","endLine":9,"endColumn":31},{"ruleId":"70","severity":1,"message":"74","line":10,"column":19,"nodeType":"72","messageId":"73","endLine":10,"endColumn":27},{"ruleId":"70","severity":1,"message":"75","line":11,"column":23,"nodeType":"72","messageId":"73","endLine":11,"endColumn":35},{"ruleId":"70","severity":1,"message":"76","line":3,"column":7,"nodeType":"72","messageId":"73","endLine":3,"endColumn":12},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"no-unused-vars","'setDisplay' is assigned a value but never used.","Identifier","unusedVar","'setWidth' is assigned a v